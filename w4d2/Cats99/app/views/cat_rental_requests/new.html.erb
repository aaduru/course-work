<h1>Create a new Rental Request</h1>

<form action=" <%=cat_rental_requests_url  %>" method="post">
<br>
<br>

  <select  name="rental_request[cat_id]" >
    <% @cats.each do |cat| %>
      <option name="rental_request[cat_id]"  value= "<%= cat.id %>" > <%= cat.name %></option>
    <% end %>
  </select>

  <br>
    <label for="start_date"> Start Date: </label>
      <input id = "start_date" type="date" name = "rental_request[start_date]">
  <br>

  <br>
    <label for="end_date"> End Date: </label>
      <input id = "end_date" type="date" name = "rental_request[end_date]">
  <br>
  <!-- <button >Submit</button> -->
  <input type ="submit" value = "Add Request">
</form>
